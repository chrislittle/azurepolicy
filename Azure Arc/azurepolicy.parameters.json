{
    "proxyurl": {
      "type": "String",
      "metadata": {
        "displayName": "Proxy URL",
        "description": "The URL of the proxy server to use for the extension."
      }
    },
    "effect": {
      "type": "String",
      "metadata": {
        "displayName": "Effect",
        "description": "Enable or disable the execution of the policy."
      },
      "allowedValues": [
        "DeployIfNotExists",
        "Disabled"
      ],
      "defaultValue": "DeployIfNotExists"
    }
  }